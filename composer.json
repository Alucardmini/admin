{
    "name": "pingpong/admin",
    "description": "Simple Administrator Package for Laravel 4",
    "keywords": ["pingpong", "admin", "laravel", "administrator"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Pingpong Labs",
            "email": "pingpong.labs@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": "4.*",
        "illuminate/support": "4.*",
        "pingpong/menus": "1.0.*",
        "pingpong/trusty": "1.0.*",
        "pingpong/validator": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.1",
        "way/generators": "2.*",
        "pingpong/testing": "1.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "Pingpong\\Admin\\": "src/Pingpong/Admin",
            "Pingpong\\Admin\\Testing\\": "tests/Pingpong/Admin/Testing"
        }
    },
    "minimum-stability": "stable"
}
